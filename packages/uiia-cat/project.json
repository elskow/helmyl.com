{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "uiia-cat",
  "sourceRoot": "packages/uiia-cat/src",
  "projectType": "library",
  "tags": ["type:lab"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "parcel build src/index.html --dist-dir ../../apps/static/labs/uiia-cat --public-url /labs/uiia-cat/ --no-cache",
        "cwd": "packages/uiia-cat"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "parcel src/index.html --port 3004",
        "cwd": "packages/uiia-cat"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["rm -rf dist", "rm -rf ../../apps/static/labs/uiia-cat"],
        "cwd": "packages/uiia-cat",
        "parallel": false
      }
    }
  }
}
