{
	"name": "apps",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"generate:labs": "tsx scripts/generate-labs-metadata.ts",
		"dev": "pnpm generate:labs && vite dev",
		"build": "pnpm generate:labs && mkdir -p static/labs && vite build && cp static/_headers build/_headers",
		"preview": "vite preview",
		"prepare": "rm -rf .tw-patch && tw-patch install && tw-patch extract",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
